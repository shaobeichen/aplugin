{
    "name": "oplugin",
    "version": "0.1.0",
    "description": "A plugin system allows you to easily enable additional functionality. ",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.cts",
    "exports": {
        "require": {
            "types": "./dist/index.d.cts",
            "default": "./dist/index.cjs"
        },
        "import": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        }
    },
    "scripts": {
        "dev": "tsx watch ./src/index.ts",
        "build": "pkgroll",
        "test": "jest",
        "type": "tsc --noEmit"
    },
    "keywords": [
        "plugin",
        "oplugin",
        "pluggable",
        "flow",
        "widget",
        "block",
        "tapable",
        "typescript",
        "tsx"
    ],
    "author": "shaobeichen <shaobeichen@outlook.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.9.0",
        "jest": "^29.7.0",
        "pkgroll": "^2.5.1",
        "ts-jest": "^29.2.5",
        "tsx": "^4.19.2",
        "typescript": "^5.6.3"
    }
}